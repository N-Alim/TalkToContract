 {% extends "admin/admin.html.twig" %} 

 {% block title %}Skills {% endblock %}


{% block body %}
    <h1>Skills</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ skills.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ skills.name }}</td>
            </tr>
            <tr>
                <th>Modified_at</th>
                <td>{{ skills.modifiedAt ? skills.modifiedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ skills.createdAt ? skills.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('skills_index_admin') }}">back to list</a>

    <a href="{{ path('skills_edit_admin', {'id': skills.id}) }}">edit</a>

    {{ include('/admin/skills/_delete_form.html.twig') }}
{% endblock %}