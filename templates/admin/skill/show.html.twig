 {% extends "admin/admin.html.twig" %} 

 {% block title %}Skill {% endblock %}


{% block body %}
    <h1>Skill</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ skill.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ skill.name }}</td>
            </tr>
            <tr>
                <th>Modified_at</th>
                <td>{{ skill.modifiedAt ? skill.modifiedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ skill.createdAt ? skill.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('skill_index_admin') }}">back to list</a>

    <a href="{{ path('skill_edit_admin', {'id': skill.id}) }}">edit</a>

    {{ include('/admin/skill/_delete_form.html.twig') }}
{% endblock %}